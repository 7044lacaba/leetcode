class Solution(object):
    def productExceptSelf(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        final = []
        for i, item in enumerate(nums):
            temp = nums.copy()
            temp.pop(i)
            value = 1
            for item in temp:
                value = value * item
            final.append(value)
        return final
    

